<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".immunocompromized"
    android:background = "#0b081A">

    <TextView
        android:id="@+id/immunocompromised"
        android:layout_width="380dp"
        android:layout_height="123dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:gravity="center_horizontal|top"
        android:text="Are you immunocompromised?"
        android:fontFamily="@font/raleway"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="173dp"></TextView>


    <TextView
        android:id="@+id/yes_immuno"
        android:layout_width="380dp"
        android:layout_height="123dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:fontFamily="@font/raleway"
        android:gravity="center"
        android:text="Yes"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="296dp"></TextView>

    <TextView
        android:id="@+id/yes_immunocompromised"
        android:layout_width="380dp"
        android:layout_height="123dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:fontFamily="@font/raleway"
        android:gravity="center"
        android:text="No"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="440dp"

        />

</androidx.constraintlayout.widget.ConstraintLayout>